  "engines": {
    "node": ">=14.16",
    "bun": ">=0.5.8"
  },
  "dependencies": {
    "@farjs/blessed": "0.2.6",
    "react": "^17.0.1",
    "react-blessed": "0.7.2"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.1",
    "@types/react-blessed": "^0.7.3",
    "@types/react-test-renderer": "^17.0.1",
    "c8": "^7.13.0",
    "mock-fn": "^1.0.0",
    "prettier": "^2.8.8",
    "react-assert": "^1.0.3",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.9.5"
  }
