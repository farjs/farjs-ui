{
  "name": "@farjs/ui",
  "author": "viktor-podzigun",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Terminal UI React.js components library",
  "scripts": {
    "test": "tsc && bun test && node ./test/all.mjs",
    "format": "prettier **/*.mjs **/*.ts --write",
    "formatCheck": "prettier **/*.mjs **/*.ts --check"
  },
  "type": "module",
  "exports": {
    "./*": "./src/*"
  },
  "private": false,
  "publishConfig": {
