  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farjs/farjs-ui.git"
  },
  "homepage": "https://github.com/farjs/farjs-ui",
  "bugs": {
    "url": "http://github.com/farjs/farjs-ui/issues"
